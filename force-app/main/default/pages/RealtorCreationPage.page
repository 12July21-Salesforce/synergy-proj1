<apex:page controller="CreateRealtor_CC" lightningStylesheets="true">
    <apex:form >
        <apex:pageMessages />
        <apex:pageBlock >
            <apex:pageBlockSection title="Enter Realtor Information">
                <apex:inputField value="{! realtor.Name }" />
                <apex:inputField value="{! realtor.Phone_Number__c }" />
                <apex:inputField value="{! realtor.Realtor_Agency__c }" />
                <apex:inputField value="{! realtor.Email__c }" />
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton action="{! saveRealtor }" value="Save" />
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>